{
  "project": {
    "name": "DeepTechX Launchpad - Supreme Architecture",
    "version": "1.0.0",
    "description": "Living Digital Entity - 24/7 Consultancy Platform for Space & Deep Tech Ventures",
    "repository": "https://github.com/deeptechx/launchpad-supreme"
  },
  "frontend": {
    "framework": {
      "name": "Next.js",
      "version": "15.x",
      "features": ["App Router", "React Server Components", "Edge Runtime"]
    },
    "language": {
      "name": "TypeScript",
      "version": "5.x",
      "strict": true
    },
    "styling": {
      "framework": {
        "name": "Tailwind CSS",
        "version": "4.x"
      },
      "components": {
        "name": "shadcn/ui",
        "version": "latest"
      },
      "themes": ["light", "dark", "space"]
    },
    "3d": {
      "core": {
        "name": "React Three Fiber",
        "version": "9.x"
      },
      "utilities": {
        "name": "@react-three/drei",
        "version": "latest"
      },
      "xr": {
        "name": "@react-three/xr",
        "version": "latest"
      },
      "postprocessing": {
        "name": "@react-three/postprocessing",
        "version": "latest"
      },
      "compression": {
        "draco": true,
        "ktx2": true
      }
    },
    "animation": {
      "ui": {
        "name": "Framer Motion",
        "version": "11.x"
      },
      "scroll": {
        "name": "GSAP",
        "version": "3.x",
        "plugins": ["ScrollTrigger"]
      },
      "3d_physics": {
        "name": "@react-spring/three",
        "version": "latest"
      }
    },
    "state": {
      "global": {
        "name": "Zustand",
        "version": "4.x"
      },
      "server": {
        "name": "TanStack Query",
        "version": "5.x"
      },
      "atomic": {
        "name": "Jotai",
        "version": "latest"
      }
    }
  },
  "ai_ml": {
    "orchestration": {
      "name": "Vercel AI SDK",
      "version": "3.x",
      "features": ["Streaming", "Multi-model", "Tool Calling"]
    },
    "vector_database": {
      "name": "Pinecone",
      "alternative": "Weaviate",
      "index_name": "deeptechx-curriculum"
    },
    "embeddings": {
      "provider": "OpenAI",
      "model": "text-embedding-3-large",
      "dimensions": 3072
    },
    "llm": {
      "primary": {
        "provider": "OpenAI",
        "model": "gpt-4o"
      },
      "secondary": {
        "provider": "Anthropic",
        "model": "claude-3-5-sonnet"
      }
    },
    "rag": {
      "chunking": ["semantic", "recursive"],
      "reranking": {
        "provider": "Cohere",
        "model": "rerank-v3"
      }
    },
    "agent_framework": {
      "name": "LangChain",
      "status": "future",
      "features": ["Multi-step reasoning", "Tool use", "Memory"]
    }
  },
  "backend": {
    "deployment": {
      "platform": "Vercel",
      "features": ["Edge Network", "Edge Functions", "Image Optimization"],
      "locations": 100
    },
    "database": {
      "primary": {
        "name": "Supabase",
        "type": "PostgreSQL",
        "version": "2.x"
      },
      "cache": {
        "name": "Upstash Redis"
      },
      "vector": {
        "name": "Pinecone"
      }
    },
    "authentication": {
      "framework": {
        "name": "Auth.js",
        "version": "5.x"
      },
      "providers": ["Google", "GitHub", "LinkedIn"],
      "tokens": "JWT with refresh"
    },
    "payments": {
      "provider": "Stripe",
      "version": "14.x",
      "features": ["Checkout sessions", "Subscriptions", "Webhooks"]
    },
    "cms": {
      "name": "Sanity",
      "version": "3.x",
      "content_types": ["Curriculum", "Blog", "Media"]
    }
  },
  "monitoring": {
    "analytics": {
